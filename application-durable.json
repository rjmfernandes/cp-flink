{
    "apiVersion": "cmf.confluent.io/v1alpha1",
    "kind": "FlinkApplication",
    "metadata": {
       "name": "durable-example"
    },
    "spec": {
       "job": {
          "jarURI": "local:///opt/flink/examples/streaming/StateMachineExample.jar",
          "parallelism": 3,
          "state": "running",
          "upgradeMode": "stateless"
       },
       "jobManager": {
          "resource": {
          "cpu": 1,
          "memory": "1048m"
          }
       },
       "taskManager": {
          "resource": {
          "cpu": 1,
          "memory": "1048m"
          }
       }
    }
}